stock IsNumeric(dest[], pos=0)
{
    new is_numeric = true;
    do
    {
        switch(dest[pos])
        {
        case '0'..'9': continue;
        default:
            {
                is_numeric = false;
                break;
            }
        }
    }
    while(dest[++pos]);

    return is_numeric;
}

