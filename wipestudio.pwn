    if(PI[targetid][pCash] < arenda) return SendMes(targetid, 0xFF0000AA, "[Ошибка] {ffffff}У Вас недостаточно денег для аренды, необходимо %d$",arenda);
    SendMes(targetid,-1,"{ffffff}Вы арендовали %s за {66cc00}%i$", g_vehicle_info[idavto - 400][VI_NAME], arenda);
    SendClientMessage(targetid, -1, "{63CB00}Чтобы вернуться в магазин пропишите {FFCD00}/cancelrc{63CB00} или {FFCD00}нажмите 'F'");
    SetPlayerVirtualWorld(targetid, 0);
    SetPlayerInterior(targetid, 0);
    Hook_GivePlayerMoney(targetid, -arenda);
    vehiclerc[targetid] = CreateVehicle(idavto, 1382.9015,-1757.5061,13.5469,358.0287, 77, 77,7200, 0);
    zavodis[targetid] = 1;
    SetVehicleData(vehiclerc[targetid], V_FUEL, 100);
    LinkVehicleToInterior(vehiclerc[targetid], 0);
    PutPlayerInVeh(targetid, vehiclerc[targetid], 0);
    UpdatePlayerData(targetid, "pCash", PI[targetid][pCash]);
    SetPVarInt(targetid, "rentrc", 1);
    return 1;
}
stock SetPlayerExtra(playerid, declare, level, reason[])
{
    if(declare != INVALID_PLAYER_ID) SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: %s.",reason,PI[declare][pNames]);
    else SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: Неизвестный.",reason);
    if((PI[playerid][pWanted] + level) < 6) PI[playerid][pWanted] += level;
    else PI[playerid][pWanted] = 6;
    SendMes(playerid,COLOR_YELLOW,"Текущий уровень розыска: %d", PI[playerid][pWanted]);
    SetPlayerWantedLevel(playerid, PI[playerid][pWanted]);
    if(declare != INVALID_PLAYER_ID) format(stringer1, sizeof(stringer1),"Рация: Cообщает: %s. Преступление: %s. Подозреваемый: %s",PI[declare][pNames],reason,PI[playerid][pNames]);
    else format(stringer1, sizeof(stringer1),"Рация: Cообщает: Неизвестный. Преступление: %s. Подозреваемый: %s",reason, PI[playerid][pNames]);
    foreach (new i : Player)
    {
        if(!IsPlayerConnected(i) || gPlayerLogged[i] == 0) continue;
        if(PI[i][pMember] == 1 || PI[i][pMember] == 2 || PI[i][pMember] == 10 || PI[i][pMember] == 21 || PI[i][pMember] == 25)
        {
            SendClientMessage(i, 0xFEBC41AA, stringer1);
        }
    }
    return 1;
}

cmd:premcar(playerid)
{
    if(PI[playerid][pPremiumCar] != 1) return SendClientMessage(playerid, COLOR_WHITE, !"Для вас нет премиум автомобиля");
    if(PI[playerid][pPayDayHad] < PI[playerid][pPremiumHours])
    {
        new scm_string[94];
        format(scm_string, sizeof(scm_string), "Необходимо отыграть %d часов чтобы забрать премиум автомобиль [У вас отыграно: %d часов]", PI[playerid][pPremiumHours], PI[playerid][pPayDayHad]);
        return SendClientMessage(playerid, COLOR_WHITE, scm_string);
    }
    BuyOwnableExcCar(playerid, 415, 1 ) ;

    PI[playerid][pPremiumHours] = 0;
    UpdatePlayerData(playerid, "pPremiumHours", PI[playerid][pPremiumHours]);

    PI[playerid][pPremiumCar] = 0;
    UpdatePlayerData(playerid, "pPremiumCar", PI[playerid][pPremiumCar]);
    return 1;
}

