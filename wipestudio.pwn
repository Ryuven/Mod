cmd:rang(playerid, params[])
{
    if(!PI[playerid][pMember]) return 1;
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][2]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][2]);
    if(sscanf(params, "dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /rang [id] [ранг]");
    switch(PI[playerid][pMember])
    {
    case 1,10,3,19,21: if(params[1] > 14 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 14!");
    case 2,26,4,5,6,11,12,13,14,15,17,18,22,24,25,7: if(params[1] > 9 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!");
    case 27,28: if(params[1] > 4 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!");
    case 16: if(params[1] > 12 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 12!");
    case 23: if(params[1] > 11 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 11!");
    }
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    switch(PI[playerid][pMember])
    {
    case 1..4, 7, 10, 11, 19, 21, 25:
        {
            if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
            if(PI[params[0]][pRank] <= 5 && params[1] > 5)
            {
            //    if(PI[params[0]][pMedCard] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Для получения данного ранга, у игрока должна быть медицинская карта.");
                if((PI[params[0]][pMember] == 1 || PI[params[0]][pMember] == 2 || PI[params[0]][pMember] == 10 || PI[params[0]][pMember] == 21 || PI[params[0]][pMember] == 25) && PI[params[0]][pVoenBill] == 0)
                {
                    return SendClientMessage(playerid,COLOR_WHITE,"Для получения данного ранга, у игрока должен быть военный билет. Купит его он может в /donaterub");
                }
            }
        }
    }
    if((PI[params[0]][pMember] == 3 || PI[params[0]][pMember] == 19) && params[1] >= 3 && PI[params[0]][pVoenBill] == 0)
    {
        PI[params[0]][pVoenBill] = 1;
        SendClientMessage(params[0], COLOR_GRAD1, "Поздравляем! Вы получили военный билет!");
        format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `voenbill` = '1' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, mysql_query_, false, "", "");
    }
    if (PI[params[0]][pLeader] >= 1) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы указали ID лидера");
    if(PI[playerid][pMember] != PI[params[0]][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Данный игрок не в вашей фракции!");
    if(PI[playerid][pMember] > 0 && PI[playerid][pRank] <= params[1]) return SendClientMessage(playerid,COLOR_GREY,"Ошибка");
    if(PI[playerid][pLeader] <= 0 && PI[playerid][pRank] <=  PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "Ошибка");
    format(stringer1, sizeof(stringer1), "Вас повысил/понизил до %d ранга %s",params[1], PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6BB3FFAA, stringer1);
    format(stringer1, sizeof(stringer1), "Вы повысили %s до %d ранга.", PI[params[0]][pNames],params[1]);
    SendClientMessage(playerid, 0x6BB3FFAA, stringer1);
    PI[params[0]][pRank] = params[1];
    return 1;
}

cmd:uninvite(playerid, params[])
{

    if(!PI[playerid][pMember]) return 1;
    switch(PI[playerid][pMember]) { case 1..4, 7, 10, 11, 19, 21, 25: if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!"); }
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][1]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][1]);
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_GRAD2, "Используйте: /uninvite [id] [причина]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(PI[playerid][pRank] == PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы не можете уволить этого игрока");
    if(PI[playerid][pLeader] <= 0 && PI[playerid][pRank] <= PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы не можете уволить этого игрока");
    if(PI[params[0]][pMember] == PI[playerid][pLeader] || PI[params[0]][pMember] == PI[playerid][pMember])
    {
        if(PI[params[0]][pMember] <= 0 || PI[params[0]][pLeader] >= 1) return 1;
        if(IsPlayerStartCroupier(params[0]))
        {
            if(CroupierTable[params[0]] != -1)
            {
                switch(CroupierGame[params[0]])
                {
                case 1: OnPlayerExitDiceTable(params[0], CroupierTable[params[0]]);
                case 2: OnPlayerExitRouleTable(params[0], CroupierTable[params[0]]);
                case 3: OnPlayerExitBlackJackTable(params[0], CroupierTable[params[0]]);
                }
            }
            StartDuty[params[0]] = 0;
        }
        format(stringer1, sizeof(stringer1), "%s выгнал вас из организации. Причина: %s",PI[playerid][pNames],params[1]);
        SendClientMessage(params[0], 0x6495EDFF, stringer1);
        SetPlayerSkinEx(params[0], PI[params[0]][pChar]);
        format(stringer1, sizeof(stringer1), "Вы выгнали %s из организации с причиной: %s",PI[params[0]][pNames],params[1]);
        SendClientMessage(playerid, 0x6495EDFF, stringer1);
        SendClientMessage(params[0], COLOR_WHITE, "Теперь вы обычный гражданин...");
        SetPlayerArmour(params[0],0);
        ResetWeapons(params[0]);
        PI[params[0]][pMember] = 0;
        PI[params[0]][pLeader] = 0;
        PI[params[0]][pRank] = 0;
        SetPlayerColor(params[0], 0xFFFFFF80);
        SetPlayerSkinEx(params[0], PI[params[0]][pChar]);
        format(stringer1, sizeof(stringer1), "[A] Игрок %s[%d] уволил из организации %s[%d]. Причина: %s", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0], params[1]);
        SendAdminMessage(COLOR_GREY,stringer1);
        PI[params[0]][pOdet] = 0;
        format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `odet` = '0' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, stringer, false, "", "");
    }
    return 1;
}
