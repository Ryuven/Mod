stock MafiaTimeCapture(faction, MafiaNapadss)
{
    SendFamilyMessage(faction, 0xFF0000FF, "Внимание, стрела началась!");
    SendFamilyMessage(MafiaNapadss, 0xFF0000FF, "Внимание, стрела началась!");
