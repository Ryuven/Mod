                    SendClientMessage(params[0], 0xbc2c2caa, "Используйте клавишу 'Вверх,вниз' (W,S по умолчанию)");
                    SendClientMessage(params[0], COLOR_GREY, "Если вы случайно нажали 'Enter' и меню пропало, нажмите 'Enter' еще раз");
                    SetPlayerInteriorEx(params[0],5);
                    SetPlayerVirtualWorldEx(params[0],1);
                    //ShowMenuForPlayer(ChoseSkin,params[0]);
                    new rand = random(sizeof(gInviteSpawns));
                    SetPPos(params[0], gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]); // Warp the player
                    SetPlayerFacingAngle(params[0], gInviteSpawns[rand][3]);
                    SetPlayerCameraPos(params[0],gInviteSpawns[rand][0] + 3, gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
                    SetPlayerCameraLookAt(params[0],gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
                    TogglePlayerControllable(params[0], 0);
                    SelectChar[params[0]] = INVALID_PLAYER_ID;
                    PI[params[0]][pJob] = 0;
                    SelectCharID[params[0]] = PI[params[0]][pMember];
                    SelectCharPlace[params[0]] = 1;
                    PI[params[0]][pModel] = InviteSkin[params[0]];
                    SetPlayerSkinEx(params[0], InviteSkin[params[0]], true);
                    format(stringer1, sizeof(stringer1), "(Агент ФБР) %s[%d] перевел в армию игрока %s[%d].", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0]);
                    SendAdminMessage(COLOR_GREY,stringer1);
                    return 1;
                }
                PI[params[0]][pRank] = 0;
                PI[params[0]][pMember] = 0;
                PI[params[0]][pLeader] = 0;
                if(!PI[params[0]][pAdmin]) PI[params[0]][pVigs] = 0;
                SetPlayerArmour(params[0],0);
                SpawnPlayerEx(params[0]);
                ResetWeapons(params[0]);
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s уволил вас из армии", PI[playerid][pNames]);
                SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "Вы снова гражданский", PI[playerid][pNames]);
                SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "Вы уволили %s из армии.", PI[params[0]][pNames]);
                SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "(Агент ФБР) %s[%d] уволил %s[%d].", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0], params[1]);
                SendAdminMessage(COLOR_GREY,stringer1);
                PI[params[0]][pOdet] = 0;
                format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `odet` = '0', `vigs` = '0' WHERE `pID` = '%d'",PI[params[0]][pID]);
                mysql_function_query(connects, stringer, false, "", "");
                return 1;
            }
            PI[params[0]][pRank] = rankdemote;
            format(stringer1, sizeof(stringer1), "Агент ФСБ %s понизил вас до %d ранга", PI[playerid][pNames], rankdemote);
            SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
            format(stringer1, sizeof(stringer1), "Вы понизили %s до %d ранга.", PI[params[0]][pNames],rankdemote);
            SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
        }
    }
    return 1;
}
