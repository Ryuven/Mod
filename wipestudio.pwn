CMD:makeyt(playerid, params[])
{
    if(!FD(playerid)) return 1;
    if(sscanf(params, "dd", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, !"Используйте: /makeyt [id игрока] [уровень]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, !"Игрок не найден.");
    if(params[1] > 2 || params[1] < 0) return SendClientMessage(playerid, COLOR_WHITE, !"Уровень может быть от 0 до 2.");

    new scm_string[144];
    if(params[1] == 0)
    {
        format(scm_string, sizeof(scm_string), "%s[%d] отобрал у Вас блогерскую адмиинку.", PI[playerid][pNames], playerid);
        SendClientMessage(params[0], COLOR_WHITE, scm_string);

        PI[params[0]][pYtADM] = 0;
        UpdatePlayerData(params[0], "pYTADM", PI[params[0]][pYtADM]);
    }
    else
    {
        format(scm_string, sizeof(scm_string), "%s[%d] назначил Вас блогером (Уровень: %d).", PI[playerid][pNames], playerid, params[1]);
        SendClientMessage(params[0], COLOR_WHITE, scm_string);

        PI[params[0]][pYtADM] = params[1];
        UpdatePlayerData(params[0], "pYTADM", PI[params[0]][pYtADM]);
    }
    return 1;
}

