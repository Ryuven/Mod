        if(PI[params[0]][pRank] < 10 || PI[params[0]][pRank] > 1)
        {
            new rankdemote = PI[params[0]][pRank];
            rankdemote -= 1;
            if(rankdemote == 0)
            {
                if(PI[params[0]][pMember] == 1 || PI[params[0]][pMember] == 10 || PI[params[0]][pMember] == 21 || PI[params[0]][pMember] == 25)
                {
                    PI[params[0]][pMember] = 19;
                    PI[params[0]][pRank] = 3;
                    InviteSkin[params[0]] = 287;
                    ResetWeapons(params[0]);
                    format(stringer1, sizeof(stringer1), "Агент ФСБ %s перевел вас обратно в армию", PI[playerid][pNames]);
                    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                    format(stringer1, sizeof(stringer1), "Вы перевели %s обратно в армию.", PI[params[0]][pNames],rankdemote);
                    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
