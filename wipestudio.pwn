publics: minute_timer()
{
    GlobalMinuteTimer = SetTimer("minute_timer", 60000, false);

    for(new i = 1; i < TOTALBIZZ + 1; i++)
    {
        if(BizzInfo[i][bMoney] >= 1000000000) BizzInfo[i][bMoney] = 1000000000;
    }

    if(settings_name != 0)
    {
        switch(random(3))
        {
        case 0: if(NameEnabled[0] > 0) SendRconCommand(ServerName[0]);
        case 1: if(NameEnabled[1] > 0) SendRconCommand(ServerName[1]);
        case 2: if(NameEnabled[2] > 0) SendRconCommand(ServerName[2]);
        }
    }

    new newhour, newminute;
    gettime(newhour, newminute);

    if(newminute == 57)
    {
        mysql_tquery(connects, "SELECT `fam_name`, `fam_rating` FROM `family_system` ORDER BY `family_system`.`fam_rating` DESC LIMIT 1", "check_message");
    }

    if ((newhour > ghour) || (newhour == 0 && ghour == 23))
    {
        new time[64];
        format(time,sizeof(time), "{FFFFFF}Текущее время: {3399FF}%d:00",newhour);
        SendAdminMessage(COLOR_LIGHTRED,time);
        ghour = newhour;
        PayDay();
        SetWorldTime(newhour);
    }

    if(newhour == 4 && newminute == 00)
    {
        SendClientMessageToAll(0xFF0000FF, !"Через 10 минут произойдёт глобальный рестарт, не забудьте завершить работы!");
    }

    if(newhour == 4 && newminute == 09)
    {
        save_server_info();
        SendClientMessageToAll(0xFF0000FF, !"Через 1 минуту произойдёт глобальный рестарт, не забудьте завершить работы!");
    }

    if(newminute == 17 && newminute == 55)
    {
        SendClientMessageToAll(0xFF0000FF, !"Через 5 минут начнётся аукцион на контейнеры. {afafaf}[/gps - > 9. Контейнеры]");
    }

    if(newminute == 18 && newminute == 00)
    {
        SendClientMessageToAll(0xFF0000FF, !"Торги за контейнеры начались. {afafaf}[/gps - > 9. Контейнеры]");
    }

    switch(newminute)
    {
    case 12,24,36,48,59:
        {
            RandomReklama(random(3));
        }
    	}

    foreach(new i : Player)
    {
        if(!IsPlayerConnected(i)) continue;

        if(PI[i][pCash] > 1000000000)
        {
            PI[i][pCash] = 1000000000;
            SendClientMessage(i, COLOR_WHITE, !"ДОСТИГНУТ ЛИМИТ ДЕНЕЖНЫХ СРЕДСТВ.");
        }

        if(PI[i][pBank] > 1000000000)
        {
            PI[i][pBank] = 1000000000;
            SendClientMessage(i, COLOR_WHITE, !"ДОСТИГНУТ ЛИМИТ ДЕНЕЖНЫХ СРЕДСТВ НА БАНК. СЧЁТЕ.");
        }
    }

    return 1;
}

publics: second_timer()
{
    GlobalSecondTimer = SetTimer("second_timer", 1000, false);

    if (BizWarTime[0] > 0)
    {
        BizWarTime[0]--;
        if (BizWarTime[0] == 180)
        {
            SendFamilyMessage(MZInfo[bFrakVlad], COLOR_LIGHTRED, "Приготовьтесь! До начала встречи осталось 3 минуты");
            SendFamilyMessage(MZInfo[bNapad], COLOR_LIGHTRED, "Приготовьтесь! До начала встречи осталось 3 минуты");
        }
        if (BizWarTime[0] == 60)
        {
            SendFamilyMessage(MZInfo[bFrakVlad], COLOR_LIGHTRED, "Внимание! До начала встречи осталась 1 минута");
            SendFamilyMessage(MZInfo[bNapad], COLOR_LIGHTRED, "Внимание! До начала встречи осталась 1 минута");
        }
        if (BizWarTime[0] == 0) MafiaTimeCapture(MZInfo[bFrakVlad], MZInfo[bNapad]);
    }
    if (BizWarTime[1]) BizWarTimer();

    //  PlantTimer();

    new hour, minute, second;
    gettime(hour, minute, second);
    foreach(new i : Player)
    {
        if(!IsPlayerConnected(i)) continue;

        new vid = GetPlayerVehicleID(i);

        if(IsPlayerInAnyVehicle(i) && GetPlayerState(i) == 2)
        {
            if(!IsAPlane(vid) && !IsABoat(vid) && !IsANoSpeed(vid))
            {
            for(new j = 0; j < 15; j ++) PlayerTextDrawShow(i, sr_carspeed_PTD[i][j]);
            UpdateSpeedometr(i);
            }
        }

        if(hour == 4 && minute == 0 && second < 5) SendClientMessageToAll(COLOR_WHITE, !"{FF0000}Через 5 минут произойдёт глобальный рестарт сервера!");
        if(hour == 4 && minute == 5 && second < 15) SendRconCommand("exit");

        if(PI[i][pCash] < -500000) PI[i][pCash] = 1000000000;
        if(PI[i][pBank] < -500000) PI[i][pBank] = 1000000000;

        if(PI[i][pGivePizzaTimer] > 0)
        {
            PI[i][pGivePizzaTimer]--;

            DisablePlayerCheckpoint(i);
            PicCP[i] = 0;

            new scm_string[16];
            format(scm_string, sizeof(scm_string), "~g~%d", PI[i][pGivePizzaTimer]);
            GameTextForPlayer(i, scm_string, 1000, 4);

            if(PI[i][pGivePizzaTimer] <= 0 && GetPVarInt(i, "work_eda_house") != 1)
            {
                PI[i][pGivePizzaTimer] = 0;

                SPD(i, 0000, DIALOG_STYLE_MSGBOX, !"{FFD700}Яндекс Еда", !"{ffffff}Отлично.\nТеперь вам нужно отвезти заказ клиенту!\nПоторопитесь, пока еда не остыла!\n\n{FF4500}Метка отмечена красным цветом на карте.", !"ОК", !"");
                TogglePlayerControllable(i, 1);
                new Float:distance = 1000000.0, house, Float:pX, Float:pY, Float:pZ;
                GetPlayerPos(i, pX, pY, pZ);
                for(new j = random(20); j < 70; j++)
                {
                    if(GetPlayerDistanceFromPoint(i, HouseInfo[j][hEntrancex], HouseInfo[j][hEntrancey], HouseInfo[j][hEntrancez]) < distance)
                    {
                        distance = GetPlayerDistanceFromPoint(i, HouseInfo[j][hEntrancex], HouseInfo[j][hEntrancey], HouseInfo[j][hEntrancez]);
                        house = j;
                    }
                }
                SetPlayerCheckpoint(i, HouseInfo[house][hEntrancex], HouseInfo[house][hEntrancey], HouseInfo[house][hEntrancez], 10.0);

                SetPVarInt(i, "work_eda_house", 1);
            }

            if(PI[i][pGivePizzaTimer] <= 0 && GetPVarInt(i, "work_eda_house_done") == 1)
            {
                PI[i][pGivePizzaTimer] = 0;

                DisablePlayerCheckpoint(i);
                PicCP[i] = 0;

                PI[i][pZakazov]++;
                TogglePlayerControllable(i, 1);
                new scm_string_2[356];
                if(PI[i][pZakazov] <= 20)
                {
                    format(scm_string_2, sizeof(scm_string_2), "\
                    {ffffff}Клиент оценил вашу работу на: {32CD32}%d баллов!\n\
                    {ffffff}Вы заработали: {32CD32}1.000.000 руб.\n\
                    {ffffff}Выполнено: {32CD32}%d/20 заказов\n\nЕзжайте за пиццей, и отвезите её клиенту.\n\n{FF0000}Метка отмечена красным цветом на карте.", random(5) + 5, PI[i][pZakazov]);

                    /*
                    if(PI[i][pZakazov] == 20)
                    {
                        new random_rub = RandomEx(10, 40);
                        PI[i][pRub] += random_rub;

                        new str_frmt[128];
                        format(str_frmt, sizeof(str_frmt), "{6495ED}Поздравляю! Вы получили {7CFC00}%d рублей. 6495ED}Продолжайте работать в том же духе!", random_rub);
                        SendClientMessage(i, COLOR_WHITE, str_frmt);
                        SendClientMessage(i, COLOR_WHITE, "{6495ED}Рубли можете потратить на админку (если хватает) {7CFC00}/donate");
                    }
                    */
                }
                else
                {
                    format(scm_string_2, sizeof(scm_string_2), "\
                    {ffffff}Клиент оценил вашу работу на: {32CD32}%d баллов!\n\
                    {ffffff}Вы заработали: {32CD32}1.000.000 руб.\n\
                    {6495ED}Выполняйте заказы, иногда мы даем за это бонусы!\n\nЕзжайте за пиццей, и отвезите её клиенту.\n\n{FF0000}Метка отмечена красным цветом на карте.", random(5) + 5);
                }
                SPD(i, 0000, DIALOG_STYLE_MSGBOX, !"{FFD700}Яндекс Еда", scm_string_2, !"ОК", !"");

                PI[i][pCash] += 1000000;
                OnPlayerUpdateRL(i);

                SetPVarInt(i, "work_eda_house", 0);
                DeletePVar(i, "work_eda_house_done");

                new Float:distance = 1000000.0, biz = -1, Float:pX, Float:pY, Float:pZ;
                GetPlayerPos(i, pX, pY, pZ);
                for(new j = 0; j < sizeof(BizzInfo); j++)
                {
                    if(BizzInfo[j][bType] == 3)
                    {
                        if(GetPlayerDistanceFromPoint(i, BizzInfo[j][bEntrx], BizzInfo[j][bEntry], BizzInfo[j][bEntrz]) < distance)
                        {
                            distance = GetPlayerDistanceFromPoint(i, BizzInfo[j][bEntrx], BizzInfo[j][bEntry], BizzInfo[j][bEntrz]);
                            biz = j;
                        }
                    }
                }
                if(biz != -1) SetPVarInt(i, "work_eda", biz), SetPlayerCheckpoint(i, BizzInfo[biz][bEntrx], BizzInfo[biz][bEntry], BizzInfo[biz][bEntrz], 10.0);
                return 1;
            }
        }

        if(PI[i][pUgonTimer] > 0)
        {
            PI[i][pUgonTimer]--;

            new scm_string[32];
            format(scm_string, sizeof(scm_string), "~r~Time: %02i:%02i", (PI[i][pUgonTimer]/60), PI[i][pUgonTimer] - (60 * (floatround(PI[i][pUgonTimer])/60)));
            GameTextForPlayer(i, scm_string, 1000, 4);

            if(PI[i][pUgonTimer] <= 0)
            {
                stringer1[0] = EOS;
                SendClientMessage(i, COLOR_YELLOW, "[SMS от Володи]: Ты не справился с заданием, нас чуть не засекли.");

                DestroyVehicle(PI[i][pUgonVehicle]);

                PI[i][pUgonTimer] =
                PI[i][pUgonVehicle] = 0;

                DisablePlayerCheckpoint(i);
            }
        }
        if(IsAGreenZone(i))
        {
            if(GetPVarInt(i, "TD_grshowed") != 1)
            {
                TextDrawShowForPlayer(i, greenzone);
                SetPVarInt(i, "TD_grshowed", 1);
            }
        }
        else
        {
            if(GetPVarInt(i, "TD_grshowed") == 1)
            {
                DeletePVar(i, "TD_grshowed");
                TextDrawHideForPlayer(i, greenzone);
            }
        }

        new CheckGun[13], CheckAmmo[13];
        for(new j = 0; j < 13; j ++)
        {
            GetPlayerWeaponData(i, j, CheckGun[j], CheckAmmo[j]);
            if(CheckGun[j] == 38)
            {
                format(stringer1, sizeof(stringer1), "{0095FF}[ANTICHEAT] {FF4500}%s[%d]{FFFFFF} был кикнут по подозрению в использовании чит-программ: {FF4500}WEAPON HACK.", PI[i][pNames], i);
                SendAdminMessage(-1, stringer1);
                KickEx(i);
                break;
            }
        }

        if(PI[i][pCash] != GetPlayerMoney(i))
        {
            ResetPlayerMoney(i);
            GivePlayerMoney(i, PI[i][pCash]);
        }

        onlinecheck[i]++;
        if(onlinecheck[i] >= 60 && PI[i][AFK_Time] < 2)
        {
            PI[i][pDayOnline]++;
            onlinecheck[i] = 0;
        }
        if(PI[i][AFK_Time] >= 3)
        {
            new string[144];
            format(string,144,"Игрок на паузе: %s",Convert(PI[i][AFK_Time]-2));
            SetPlayerChatBubble(i,string,-1,10.0,1600);
        }
        if(PI[i][AFK_Tick] > 10000) PI[i][AFK_Tick] = 1, PI[i][AFK_Check] = 0;
        if(PI[i][AFK_Check] < PI[i][AFK_Tick] && GetPlayerState(i)) PI[i][AFK_Check] = PI[i][AFK_Tick], PI[i][AFK_Time] = 0;
        if(PI[i][AFK_Check] == PI[i][AFK_Tick] && GetPlayerState(i))
        {
            PI[i][AFK_Time] += 1;
            if(PI[i][AFK_Time] > 2)
            {
                afkcheck[i]++;
                if(afkcheck[i] >= 60) {
                    PI[i][pDayAfk]++;
                    afkcheck[i] = 0;
                }
                format(stringer1, 30,"Игрок на паузе: %s",Convert(PI[i][AFK_Time]-2));
                SetPlayerChatBubble(i,stringer1,-1,30.0,3000);
            }
        }

        if(PI[i][pJailed] > 0)
        {
            if(PI[i][pJailTime] <= gettime())
            {
                SetPPos(i,2580.6282,-2419.1113,21.9574);//Тюрьма
                SetPlayerFacingAngle(i, 268.3480);

                SetPlayerInteriorEx(i, 0);
                PI[i][pJailed] = 0;
                PI[i][pJailTime] = 0;
                PI[i][pMestoJail] = 0;
                PI[i][pWanted] = 0;
                PI[i][pUsedJail] = 0;
                SetPlayerWantedLevel(i, 0);
                SetPlayerVirtualWorldEx(i, 0);
                GameTextForPlayer(i, "~g~Freedom", 5000, 1);
                SetPlayerToTeamColor(i);
                SendClientMessage(i, COLOR_ISPOLZUY,"Вы отбыли свой срок и можете идти на свободу.");
            }
        }
    }
    return 1;
}
