cmd:vips(playerid) {
    if(PI[playerid][pDonateRank] < 1) return 1;
    new string[144], j = 0;
    foreach(Player, i) {
        if(PI[i][pDonateRank] < 1 || playerid == i) continue;
        format(string, sizeof(string), "%s[%d] - %d донат ранг", PI[i][pNames], i, PI[i][pDonateRank]);
        SendClientMessage(playerid, COLOR_YELLOW, string), j++;
    }
    if(j < 1) SendClientMessage(playerid, COLOR_YELLOW, "Игроков с VIP статусом, нет в игре!");
    return 1;
}
