CMD:addseatdown(playerid)
{
    if(!FD(playerid)) return 1;
	GetPlayerPos(playerid, SeatDownX[TOTALDOWNS], SeatDownY[TOTALDOWNS], SeatDownZ[TOTALDOWNS]);
	GetPlayerFacingAngle(playerid, SeatDownA[TOTALDOWNS]);

	static const mysql_str[] = "insert into `seatdowns` (`PosX`, `PosY`, `PosZ`, `PosA`) values ('%f', '%f', '%f', '%f')";
	new str[sizeof(mysql_str) + (10*4)];

	format(str, sizeof(str), mysql_str, SeatDownX[TOTALDOWNS], SeatDownY[TOTALDOWNS], SeatDownZ[TOTALDOWNS], SeatDownA[TOTALDOWNS]);
	mysql_query(connects, str, false);

	SendClientMessage(playerid, -1, "Вы успешно создали новую посадку!");

	Create3DTextLabel("Нажмите {33aa33}[ ALT ]\n"SERVER"Чтобы сесть", -1, SeatDownX[TOTALDOWNS], SeatDownY[TOTALDOWNS], SeatDownZ[TOTALDOWNS], -1, -1);

	TOTALDOWNS++;
	return true;
}

