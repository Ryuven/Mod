cmd:sms(playerid, params[])
{
    if(PI[playerid][pPayDayHad] < 2) return SendClientMessage(playerid, COLOR_GREY, "Вы должны пробыть в игре 2 часа (/mm - Статистика персонажа).");
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /sms [id] [текст]");
    if(gag[playerid] == 1) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: У вас стоит кляп");
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, COLOR_LIGHTRED, "У вас бан чата!");
    if(takephone[playerid] == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас нет телефона");
    if(strlen(params[1]) > 95) return SendClientMessage(playerid,COLOR_GREY,"Сообщение слишком длинное!");
    if(IsAIP(params[1])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    switch (params[0])
    {
    case 11888:
        {
            if(smsls == 0) return SendClientMessage(playerid, COLOR_GREY, "Приём SMS отключен");
            if(PI[playerid][pBank] <= 0)
            {
                format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: Россия 24",params[1]);
                SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
                SendClientMessage(playerid,  COLOR_GRAD1, "Сообщение не доставлено, не достаточно средств");
                format(stringer1, sizeof(stringer1), "%s достаёт мобильник", PI[playerid][pNames]);
                SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,5.0,10000);
                return 1;
            }
            format(stringer1, sizeof(stringer1), "[Эфир] %s. Отправитель: %s[%i]",params[1],PI[playerid][pNames], playerid);
            SendFamilyMessage(16, COLOR_YELLOW, stringer1);
            format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: Россия 24",params[1]);
            SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
            PI[playerid][pMobile] -= smspricels;
            FracBank[0][fLsnews] += smspricels;
        }
    }
    if (IsPlayerConnected(params[0]))
    {
        if(params[0] != INVALID_PLAYER_ID)
        {
            if(PhoneOnline[params[0]] > 0)
            {
                SendClientMessage(playerid, COLOR_GREY, "Телефон абонента выключен...");
                format(stringer1, sizeof(stringer1), "%s достаёт мобильник", PI[playerid][pNames]);
                SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,5.0,10000);
                return 1;
            }
            if(PI[params[0]][AFK_Time] > 1) return SendClientMessage(playerid, COLOR_GREY, "Данный игрок AFK");
            PI[playerid][pBank] -=20;
            format(stringer1, sizeof(stringer1), "[Входящее SMS]: %s | Отправитель: %s[%d]",params[1],PI[playerid][pNames], playerid);
            SendClientMessage(params[0], COLOR_YELLOW, stringer1);
            SetPlayerChatBubble(params[0],"SMS <<<",COLOR_YELLOW,5.0,10000);
            foreach(Player,i)
            {
                if(BigEarSms[i] != 0)
                {
                    format(stringer1, sizeof(stringer1), "» Входящее к %s[%d]: %s. Отправитель: %s[%d].",PI[params[0]][pNames],params[0],params[1],PI[playerid][pNames],playerid);
                    SendClientMessage(i, COLOR_YELLOW, stringer1);
                }
            }
            PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
            PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
            format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: %s[%d]",params[1], PI[params[0]][pNames], params[0]);
            SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
            SetPlayerChatBubble(playerid,"SMS >>>",COLOR_YELLOW,5.0,10000);
            return 1;
        }
    }
    return 1;
}
cmd:tazer(playerid, params[])
{
    if(PI[playerid][pMember] == 2 || PI[playerid][pLeader] == 2 || IsACop(playerid)) {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Введите: /tazer [1/2/3]");
        switch(params[0])
        {
        case 1:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех",PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                        FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                        PlayerCuffed[i] = 1;
                        PlayerCuffedTime[i] = 15;
                        SetTimerEx("UnFreez", 15000, 0, "d", i);
                    }
                }
            }
        case 2:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех рядомстоящих законников",PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        if(IsAArm(i) || IsACop(i) || IsAMedic(i))
                        {
                            SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                            FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                            PlayerCuffed[i] = 1;
                            PlayerCuffedTime[i] = 15;
                            SetTimerEx("UnFreez", 15000, 0, "d", i);
                        }
                    }
                }
            }
        case 3:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех рядомстоящих жителей", PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        if(PI[i][pMember] == 0 || ISAMAFIA(i))
                        {
                            SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                            FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                            PlayerCuffed[i] = 1;
                            PlayerCuffedTime[i] = 15;
                            SetTimerEx("UnFreez", 15000, 0, "d", i);
                        }
                    }
                }
            }
        }
    }
    return 1;
}

cmd:veha(playerid, params[])
{
    if(!FD(playerid)) return true;
    if(sscanf(params, "ddd",params[0],params[1],params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    new intt = GetPlayerInterior(playerid);
    if(params[0] < 400 || params[0] > 611) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    if(params[1] < 0 || params[1] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    if(params[2] < 0 || params[2] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    new Float:X,Float:Y,Float:Z, string[144];
    GetPlayerPos(playerid, X,Y,Z);
    createdvehs ++ ;
    createdvehicles[createdvehs] = CreateVehicle(params[0], X,Y,Z, 0.0, params[1], params[2], 60000, 0, VEHICLE_ACTION_TYPE_ADMIN);
    vehcreat[createdvehicles[createdvehs]] = 1;
    PutPlayerInVeh(playerid, createdvehicles[createdvehs], 0);
    zavodis[playerid] = 1;
    LinkVehicleToInterior(createdvehicles[createdvehs], intt);
    format(string, sizeof(string), "[A] %s[%d] создал автомобиль '%s' (id %d)(color %d, %d)", PI[playerid][pNames], playerid, g_vehicle_info[params[0] - 400][VI_NAME], params[0], params[1], params[2]);
    SendAdminMessage(COLOR_GREY, string);
    format(stringer1, sizeof(stringer1), "Автомобиль ID: %d создан, для удаления /delveh", params[0]);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    return 1;
}

