cmd:contract(playerid, params[])
{
    if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /contract [id] [сумма]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PI[params[0]][pAdmin] > 0 || PI[params[0]][pMember] == 23 || PI[playerid][pMember] == 23) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете заказать данного человека.");
    if(params[1] < 10000 || params[1] > 10000000) return SendClientMessage(playerid, COLOR_GREY, "Сумма контракта должна быть минимум $10000, и максимум $1000000!");
    if(IsACop(params[0]) && params[1] < 50000) return SendClientMessage(playerid, COLOR_GREY,"Голова данного человека стоит больше чем вы предлогаете.");
    if(PI[params[0]][pContract] > gettime()) return SendClientMessage(playerid,COLOR_GREY,"Данная жертва уже была заказана, хитманы не могут её принять.");
    if(PI[playerid][pCash] < params[1]) return SendClientMessage(playerid,COLOR_GREY,"Вы решили нас обмануть? У вас нет денег!");
    PI[playerid][pCash] -= params[1];
    PI[params[0]][pHeadValue] += params[1];
    format(stringer1, sizeof(stringer1), "Внимание, поступил новый заказ цена головы заказчика %s руб..", formatInt(params[1]));
    SendFamilyMessage(23, COLOR_YELLOW, stringer1);
    SendMes(playerid,COLOR_WHITE,"Вы успешно заказали %s за %s руб..",PI[params[0]][pNames],formatInt(params[1]));
    PI[params[0]][pContract] = gettime() + 600;
    return 1;
}
